    "@types/estree": "^1.0.5",
    "@types/jasmine": "~4.3.0",
    "dts-bundle-generator": "^9.5.1",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^14.0.0",
    "typescript": "~4.7.0"
  }
